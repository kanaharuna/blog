<h3>User info</h3>
  <%= attachment_image_tag user,:profile_image,fallback:"no_image.jpg",size:'100x100' %>
  <table>
    <tbody>
      <tr>
        <th>name</th>
        <th><%= user.name %></th>
      </tr>
      <tr>
        <th>introduction</th>
        <th><%= user.introduction %></th>
      </tr>
    </tbody>
  </table>
  <%= link_to 'Edit',edit_user_path(user.id) %>